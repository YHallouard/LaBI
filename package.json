{
  "name": "hemea",
  "version": "1.2.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest",
    "test:watch": "jest --watch",
    "prepare": "husky",
    "lint": "npx eslint \"{src,apps,libs,test}/**/*.{ts,tsx}\" --fix",
    "format": "npx prettier --write \"{src,apps,libs,test}/**/*.{ts,tsx,json}\"",
    "lint-staged": "npx lint-staged",
    "test:coverage": "jest --collect-coverage",
    "mutation": "npx stryker run",
    "mutation:file": "npx stryker run --mutate",
    "postinstall": "patch-package",
    "sync-version": "node scripts/sync-version.js",
    "prebuild": "npm run sync-version"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "@mistralai/mistralai": "^1.6.0",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-community/datetimepicker": "8.3.0",
    "@react-navigation/bottom-tabs": "^7.3.10",
    "@react-navigation/native": "^7.1.6",
    "@react-navigation/stack": "^7.2.10",
    "@types/uuid": "^10.0.0",
    "buffer": "^6.0.3",
    "expo": "53.0.8",
    "expo-document-picker": "~13.1.5",
    "expo-file-system": "~18.1.8",
    "expo-image-picker": "16.1.4",
    "expo-secure-store": "~14.2.3",
    "expo-sqlite": "~15.2.9",
    "expo-system-ui": "5.0.7",
    "react": "19.0.0",
    "react-native": "0.79.2",
    "react-native-gesture-handler": "^2.25.0",
    "react-native-get-random-values": "~1.11.0",
    "react-native-gifted-charts": "^1.4.60",
    "react-native-safe-area-context": "^5.3.0",
    "react-native-screens": "~4.10.0",
    "react-native-svg": "^14",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-webview": "13.13.5",
    "react-navigation-header-buttons": "13.0.0",
    "uuid": "^11.1.0",
    "web-streams-polyfill": "^4.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@eslint/js": "9.26.0",
    "@react-native-community/cli": "latest",
    "@stryker-mutator/core": "^7.3.0",
    "@stryker-mutator/jest-runner": "^7.3.0",
    "@stryker-mutator/typescript-checker": "^7.3.0",
    "@testing-library/react-native": "^11.5.0",
    "@types/jest": "^29.5.14",
    "@types/react": "~19.0.10",
    "@types/react-native": "^0.72.8",
    "babel-plugin-transform-remove-console": "6.9.4",
    "eslint": "9.26.0",
    "eslint-plugin-react": "7.37.5",
    "globals": "16.0.0",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.0",
    "patch-package": "^8.0.0",
    "prettier": "^2.0.0",
    "react-test-renderer": "19.0.0",
    "ts-jest": "^29.3.2",
    "typescript": "~5.8.3",
    "typescript-eslint": "8.31.1"
  },
  "private": true,
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  }
}
